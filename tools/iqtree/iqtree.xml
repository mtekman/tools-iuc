<tool id="iqtree_1" name="IQtree" version="1.5.5" >
  <macros>
    <import>iqtree.macros.xml</import>
  </macros>
  <description>Efficient phylogenomic software by maximum likelihood</description>

  <expand macro="command" />
  <expand macro="inputs" />
  
  <outputs>
    <!-- Output files are given by the '-o' and '-pre' switches set to OUT and PREF, respectively -->
    <collection name="iqtree_out" type='data' >
      <discover_datasets pattern="((OUT|PREF).*)" format="text" />
    </collection>
  </outputs>

  <tests>
    <test>
      <param name='test' value='true' />
      <output name='out1' file='OUT' />
    </test>
  </tests>

  <help>
More information can be found here: http://www.iqtree.org/doc/
  </help>
</tool>
